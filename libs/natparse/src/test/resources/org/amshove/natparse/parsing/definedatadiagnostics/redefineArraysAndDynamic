2 #ARR1-2 (I4)

1 #ARR2 (I4/1:*)  /* !{D:ERROR:NPP022}
1 REDEFINE #ARR2
2 #ARR2-1 (I4)

1 #ARR3 (I4/2:4)
1 REDEFINE #ARR3 /* !{D:ERROR:NPP015}
2 #ARR3-1 (I4)
2 #ARR3-2 (I4)
2 #ARR3-3 (I4)
2 #ARR3-4 (I4)

1 #DYN1 (A) DYNAMIC
1 REDEFINE #DYN1  /* !{D:ERROR:NPP023}
2 #DYN1-1 (A1)

1 #DYN2 (B) DYNAMIC
1 REDEFINE #DYN2  /* !{D:ERROR:NPP023}
2 #DYN2-1 (B1)

1 #DYN3 (U) DYNAMIC
1 REDEFINE #DYN3  /* !{D:ERROR:NPP023}
2 #DYN3-1 (B1)

1 #DYN-MEMBER1 (A10)
